        "@types/mongoose": "^5.11.96",
        "@types/node": "^22.10.10",
        "@types/supertest": "^6.0.2",
        "@types/validator": "^13.12.2",
        "jest": "^29.7.0",
        "nodemon": "^3.1.9",
